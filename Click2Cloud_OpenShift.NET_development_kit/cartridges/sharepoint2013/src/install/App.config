<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="SharePointWebTemplate" value=""/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IFBAUser" />
        <binding name="BasicHttpBinding_ISharePoint" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:4040/FBAUser.svc" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_IFBAUser" contract="FBAMembershipService.IFBAUser"
        name="BasicHttpBinding_IFBAUser" />
      <endpoint address="http://localhost:4040/SharePoint.svc" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_ISharePoint" contract="SharePointService.ISharePoint"
        name="BasicHttpBinding_ISharePoint" />
    </client>
  </system.serviceModel>
</configuration>